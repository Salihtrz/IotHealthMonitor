@model DoctorRegisterDto
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <title>Doktor Kayıt Formu</title>

    <!-- Bootstrap -->
    <link rel="stylesheet" href="~/Dashboard/html/bootstrap/css/bootstrap.min.css">

    <!-- Google Font -->
    <link href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700" rel="stylesheet">

    <!-- Template style -->
    <link rel="stylesheet" href="~/Dashboard/html/dist/css/style.css">
    <link rel="stylesheet" href="~/Dashboard/html/pages/et-line-font/et-line-font.css">
    <link rel="stylesheet" href="~/Dashboard/html/pages/font-awesome/css/font-awesome.min.css">
    <link type="text/css" rel="stylesheet" href="~/Dashboard/html/dist/weather/weather-icons.min.css">
    <link type="text/css" rel="stylesheet" href="~/Dashboard/html/dist/weather/weather-icons-wind.min.css">

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
          <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
          <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->
</head>

<body class="body-bg-color">
    <div class="wrapper">
        <div class="form-body">
            <form method="post" class="col-form">
                <div class="col-logo"><a href="../index.html"><img alt="" src="~/Dashboard/html/dist/img/logo-lg.png"></a></div>
                <header>Doktor Kayıt Formu</header>
                <fieldset>
                    <section>
                        <div class="form-group has-feedback">
                            <label class="control-label">Ad</label>
                            <input class="form-control" asp-for="Name" placeholder="Ad" type="text">
                            <span class="fa fa-user form-control-feedback" aria-hidden="true"></span>
                        </div>
                    </section>
                    <section>
                        <div class="form-group has-feedback">
                            <label class="control-label">Soyad</label>
                            <input class="form-control" asp-for="Surname" placeholder="Soyad" type="text">
                            <span class="fa fa-envelope form-control-feedback" aria-hidden="true"></span>
                        </div>
                    </section>
                    <section>
                        <div class="form-group has-feedback">
                            <label class="control-label">Email</label>
                            <input class="form-control" asp-for="Email" placeholder="Email" type="text">
                            <span class="fa fa-envelope form-control-feedback" aria-hidden="true"></span>
                        </div>
                    </section>
                    <section>
                        <div class="form-group has-feedback">
                            <label class="control-label">Telefon</label>
                            <input class="form-control" asp-for="Phone" placeholder="Telefon" type="text">
                            <span class="fa fa-envelope form-control-feedback" aria-hidden="true"></span>
                        </div>
                    </section>
                    <section>
                        <div class="form-group has-feedback">
                            <label class="control-label">Şifre</label>
                            <input class="form-control" asp-for="Password" placeholder="Şifre" type="password">
                            <span class="fa fa-lock form-control-feedback" aria-hidden="true"></span>
                        </div>
                    </section>
                    <section>
                        <div class="form-group has-feedback">
                            <label class="control-label">Dal</label>
                            @Html.DropDownListFor(x => x.BranchID, (List<SelectListItem>)ViewBag.values, new { @class = "form-control" })
                            <span class="fa fa-lock form-control-feedback" aria-hidden="true"></span>
                        </div>
                    </section>
                    <div>
                        <label class="control-label">Uzmanlık Alanı</label>
                        <select class="form-control nice-select wide" asp-for="Degree">
                            <option value="" disabled selected>
                                Uzmanlık Alanı
                            </option>
                            <option value="Stajyer">
                                Stajyer
                            </option>
                            <option value="Pratisyen">
                                Pratisyen
                            </option>
                            <option value="Asistan">
                                Asistan
                            </option>
                            <option value="Uzman">
                                Uzman
                            </option>
                            <option value="Başasistan">
                                Başasistan
                            </option>
                            <option value="Başhekim Yardımcısı">
                                Başhekim Yardımcısı
                            </option>
                            <option value="Başhekim">
                                Başhekim
                            </option>
                        </select>
                    </div>
                </fieldset>
                <footer class="text-right">
                    <button type="submit" class="btn btn-info pull-right">Kayıt Ol</button>
                    <a href="/Login/DoctorLogin/" class="button button-secondary">Giriş Yap</a>
                </footer>
            </form>
        </div>
    </div>
    <script src="~/Dashboard/html/dist/js/jquery.min.js"></script>
    <script src="~/Dashboard/html/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/Dashboard/html/dist/js/ovio.js"></script>
</body>
</html>